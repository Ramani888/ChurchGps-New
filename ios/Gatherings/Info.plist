      <string>SF-Pro-Display-Medium.otf</string>
      <string>SF-Pro-Display-Regular.otf</string>
      <string>SF-Pro-Display-Semibold.otf</string>
      <string>Inter_24pt-Bold.ttf</string>
      <string>Inter_24pt-Medium.ttf</string>
      <string>Inter_24pt-Regular.ttf</string>
      <string>Inter_24pt-SemiBold.ttf</string>
